.COMPILE radcal
.COMPILE readjson
.COMPILE rpcortho
.COMPILE rpcredefine
.COMPILE snydov
.COMPILE subsetbyshp
.COMPILE timeex
.COMPILE unzip
.COMPILE wfv1handler
.COMPILE wfv6handler

